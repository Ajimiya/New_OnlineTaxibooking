@model New_OnlineTaxibooking.DAL.ProductModel@{    Layout = "~/Views/Shared/_Layout.cshtml";}<!-- inner page section --><section class="inner_page_head">    <div class="container_fuild">        <div class="row">            <div class="col-md-12">                <div class="full">                    <h3></h3>                </div>            </div>        </div>    </div></section><!-- end inner page section --><section class="product_section layout_padding">    <div class="container">        <div class="heading_container heading_center">            <h2>                Our <span>Cars</span>            </h2>        </div>        <div class="container">            @foreach (var c in Model.cat)            {<a href="@Url.Action("Products", "Home", new {id=c.Cat_Id })" class="btn btn-outline-danger"> @c.Cat_Name</a>}        </div>        <div class="row">            @foreach (var o in Model.pro)            {<div class="col-sm-6 col-md-4 col-lg-3">    <div class="box">        <div class="option_container">            <div class="options">                <a href="" class="option1">                    @o.Prod_Name                </a>                <a href="@Url.Action("Create","Bookings", new { id = o.Prod_Id})" class="option2">                    Book here                </a>            </div>        </div>        <div class="img-box rounded" style="background-color:aliceblue">            <img class="rounded" src="@Url.Content(o.Prod_Pic)" height="200px;" width="180px; " />;        </div>        <div class="detail-box">            <h5>                @o.Prod_Disc            </h5>        </div>        <div class="price-label">            <h6>                $ @o.Price /per day            </h6>        </div>           </div></div>}        </div>        <div class="btn-box">            <a href="@Url.Action("Index","Home")">                Back to Home            </a>        </div>    </div></section>